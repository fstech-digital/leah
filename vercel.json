{
  "functions": {
    "api/save.js": {
      "runtime": "nodejs18.x"
    },
    "api/get.js": {
      "runtime": "nodejs18.x"
    }
  },
  "env": {
    "MONGODB_URI": "@mongodb_uri",
    "SITE_URL": "https://leah-costura.vercel.app"
  },
  "rewrites": [
    {
      "source": "/api/save",
      "destination": "/api/save.js"
    },
    {
      "source": "/api/get/(.*)",
      "destination": "/api/get.js?id=$1"
    },
    {
      "source": "/api/import",
      "destination": "/api/import.js"
    },
    {
      "source": "/api/list",
      "destination": "/api/list.js"
    }
  ]
}